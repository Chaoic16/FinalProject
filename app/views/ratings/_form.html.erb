<font color="FFFFFF">
<%= form_for(@rating) do |f| %>
  <% if @rating.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@rating.errors.count, "error") %> prohibited this rating from being saved:</h2>

      <ul>
      <% @rating.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<table>
	<div class="field">
		<tr>
			<td> <%= f.label :value %> </td>
			<td> <%= f.number_field :value %> </td>
		</tr>
	</div>
	<div class="field">
		<tr>
			<td> <%= f.label :user_id %> </td>
			<td> <%= f.text_field :user_id %> </td>
		</tr>
 	</div>
 	<div class="field">
		<tr>
			<td> <%= f.label :game_id %> </td>
			<td> <%= f.text_field :game_id %> </td>
		</tr>
 	</div>
</table><br /><hr /><br />

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
</font>
